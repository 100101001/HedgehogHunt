#table-header {
  position: fixed;
  top: 0rpx;
  z-index: 100;
}
/**表格标题***/
.th {
  font-weight: bold;
  width: 20%;
  justify-content: center;
  background: #DEB887;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
}

.table {
  border: 0px solid darkgray;
  position: absolute;
  top: 100rpx;
  width: 100%;
}
/**表格行****/
.tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
/****行内单个表格元素****/
.td {
  color: #888888;
  width:20%;
  justify-content: center;
  text-align: center;
}
/**表格行背景色交替****/
.bg-w{
  background: rgba(255,228, 181, 0.4);
}
.bg-g{
  background: #FFFFF0;
}

/**新增管理员，表单输入**/
.inputbox{
  font-size: 26rpx;
  font-weight: 500;
  margin: 20rpx auto;
  border: 1px solid #f7931e;
  border-radius: 10rpx;
  width:300rpx;
  text-align: center;
}
/**新增管理员，提交表单**/
.submit-btn{
  background-color:#f7931e;
  font-size: 26rpx;
  font-weight: 600;
  text-align: center;
  color:#fff;
  border-radius: 20rpx;
  width: 200rpx;
  margin: 50rpx auto;
  margin-bottom: 0rpx;
}
/**表格最后2列的操作和查看管理员详情的按钮**/
.op-btn{
  width: 80rpx;
  height: 60rpx;
  border-radius: 10rpx;
  background-color: #f7931e;
  font-size: 26rpx;
  margin: 0 auto;
  display: flex;
  color: #fff;
  text-align: center;
  justify-content: center;
  align-items: center;
}
.grey {
  background-color: #cdcdcd;
}
.restore {
  background-color: #87CEFA;
}
/**新增管理员的按钮**/
.add-btn{
  background-color:#f7931e;
  font-size: 26rpx;
  font-weight: 600;
  text-align: center;
  color:#fff;
  position:fixed;
  left:530rpx;
  top:870rpx;
  padding: 3rpx 30rpx 3rpx 30rpx;
  border-radius: 20rpx;
}