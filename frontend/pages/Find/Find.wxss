/*}*/

.type-navbar-item {
  border-bottom: 10rpx solid #FFE4B5;
}

.type-item-on {
  border-bottom: 10rpx solid #f7931e;
  color: #f7931e;
  font-weight: 600rpx;
}

.no-more-goods {
  text-align: center;
  font-size: 24rpx;
  padding-top: 50rpx;
  padding-bottom: 228rpx;
  color: #999;
}

.school_select_area {
    height: 100%;
    width: 50%;
    position: fixed;
    z-index: 0;
}

.on{
  background-color: rgba(247, 147, 30, 0.5);
  overflow:scroll;
}

.campus{
    color: white;
    padding: 30rpx 0 30rpx 40rpx;
}
.school_select_content{
    padding-top: 16%;
    padding-bottom: 45%;
}

.all_container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  position: absolute;
  z-index: 0;
  transition: All 0.4s ease;
  -webkit-transition: All 0.4s ease;
}

.fold{
  transform: rotate(0deg) scale(1) translate(50%,0%);
  -webkit-transform: rotate(0deg) scale(1) translate(50%,0%);
}




