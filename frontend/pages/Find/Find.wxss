@import "../template/goods-item/goods-item-template.wxss";
@import "../template/navigate-bar/navigate-bar-template.wxss";

page {
/*背景图片*/
  background-image:url('data:image/jpeg;base64,');
  background-repeat:no-repeat;
  background-size:100% 100%;
  background-attachment: fixed;
}




/***让状态切换栏吸附在顶部**/
.fixed-tab{
  position: fixed;
  top: 85rpx;
  left: 0;
  right: 0;
  z-index: 10;
}

@import "../template/floating-ball/floating-ball-template.wxss";

.search_container {
  align-items: center;
  position: fixed;
  display: flex;
  flex-direction: row;
  padding: 5px;
  background-color: rgba(247,147,30, 0.5);
  width: 750rpx;
  top: 0;
  z-index:500;
}

.search_box {
  display: flex;
  justify-content: center;
  margin-left: 20rpx;
  align-items: center;
  border-radius: 25rpx;
  width: 220rpx;
  font-size: 22rpx;
  flex-direction: row;
  background-color: #fff;
  height: 50rpx;
  margin-bottom: 8rpx;
  margin-top: 8rpx;
}
.search_focus {
  box-shadow: 4rpx 4rpx 10rpx #333;
}

.search_text_box {
  margin-left: 25rpx;
  height: 35rpx;
  width: 160rpx;
  color: #f7931e;
}

.search_icon{
  margin-left:5rpx;
  color: #f7931e;
}

.clean_icon {
  margin-right: 8rpx;
  color: #cdcdcd;
}

.search_button {
  color: #fff;
  background-color: #f7931e;
  height: 50rpx;
  display: flex;
  align-items: center;
  padding-left:30rpx;
  padding-right: 30rpx;
  border: 2px #fff solid;
  border-radius: 30rpx;
  font-size: 22rpx;
  margin-left: 20rpx;
}

.message_container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  z-index: 1;
}

.swiper-container {
  width: 750rpx;
  /*  position: relative;*/
  margin-top: 65rpx;
}

.swiper_box {
  width: 100%;
  height: 300rpx;
}

swiper-item image {
  width: 100%;
  display: inline-block;
  overflow: hidden;
  height: 300rpx;
}

.swiper-container .dots {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 20rpx;
  display: flex;
  justify-content: center;
}

.swiper-container .dots .dot {
  margin: 0 8rpx;
  width: 14rpx;
  height: 14rpx;
  background: #fff;
  border-radius: 50%;
  transition: all 0.6s;
  opacity: 0.5;
}

.swiper-container .dots .dot.active {
  width: 14rpx;
  opacity: 1;
}


.type-navbar {
  white-space: nowrap;
  height: 64rpx;
  width: 100%;
  background-color: #FFE4B5;
}

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.type-box {
  width: 185rpx;
  box-sizing: content-box;
  font-size: 26rpx;
  color: #A0522D;
  letter-spacing: 3rpx;
  font-weight: bold;
  line-height: 60rpx;
  text-align: center;
  display: inline-block;
  overflow: hidden;
}

.type-box-five {
  width: 150rpx;
}
/*.status-three{*/
