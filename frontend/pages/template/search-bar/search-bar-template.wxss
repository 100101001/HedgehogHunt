
.search_container {
  align-items: center;
  position: fixed;
  display: flex;
  flex-direction: row;
  padding: 5px;
  background-color: rgba(247,147,30, 0.5);
  width: 750rpx;
  justify-content: space-between;
  top: 0;
  z-index:500;
}

.search_box1 {
  margin-left: 30rpx;
  align-items: center;
  border-radius: 25rpx;
  width: 200rpx;
  font-size: 22rpx;
  display: flex;
  flex-direction: row;
  background-color: #fff;
  height: 50rpx;
  margin: 8rpx auto;
}

.search_box2 {
  margin-left: 10rpx;
  align-items: center;
  border-radius: 25rpx;
  width: 200rpx;
  font-size: 22rpx;
  display: flex;
  flex-direction: row;
  background-color: #fff;
  height: 50rpx;
  margin: 8rpx auto;
}
.search_box_focus {
 box-shadow: 4rpx 4rpx 10rpx #333333;
}
