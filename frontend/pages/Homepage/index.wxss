/* pages/Homepage/index.wxss */
@import "../template/navigate-bar/navigate-bar-template.wxss";
page {
  background-image:url('data:image/jpeg;base64,');
  background-repeat:no-repeat;
  background-size:100% 100%;
  background-attachment: fixed;
}


.invite-tutorial {
  background-color: rgba(51,51,51, 0.75);
  position: fixed;
  top:0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 100;
}
#invite_finger {
  position: fixed;
  top: 3%;
  left: 76%;
  width: 100rpx;
  height: 100rpx;
  animation: hint 3s 1 ease-in-out;
}

@keyframes hint {
  0% {top: 50%; left: 50%;}
  20% {top: 3%; left: 76%;}
  25% {transform: scale(1);}
  30% {transform: scale(1.1);}
  35% {transform: scale(1);}
  40%{transform: scale(1.1);}
  45% {transform: scale(1);}
  50% {transform: scale(1.1);}
  55% {transform: scale(1);}
  60%{transform: scale(1.1);}
  65% {transform: scale(1);}
  70% {transform: scale(1.1);}
  75% {transform: scale(1);}
  80%{transform: scale(1.1);}
}


.official-account {
 align-items: center;
 margin-left: -25rpx;
}
.homepage_container {
 position: absolute;
}
.blank-placeholder {
  height: 200rpx;
}
/****头像，昵称，积分框****/
.user-info-container {
 height: 200rpx;
 opacity: 0.8;
 display: flex;
 flex-direction: column;
 background: linear-gradient(to right, #FFDEAD, #f7931e);
}

/****头像，昵称栏****/
.user-avatar-container{
  display: flex;
  flex-direction: row;
}

.user-info-avatar {
  margin-left: 35rpx;
  margin-top: 15rpx;
  width: 120rpx;
  height: 120rpx;
  border-radius: 60rpx;
}

.user-info-nickname{
   margin-top: 50rpx;
   margin-left: 30rpx;
   color: #fff;
   font-size: 30rpx;
   letter-spacing: 3rpx;
   font-weight: bold;
   justify-content: center;
}

/****积分条****/
.user-level-progress{
  padding-left: 25rpx;
  width: 600rpx;
  height: 10rpx;
  margin-top: 10rpx;
  margin-bottom: 20rpx;
}
.user-level-text{
   color: #fff;
   margin-top:10rpx;
   font-size: 24rpx;
   font-weight: bold;
}

/****权益提示****/
.user-info-hint{
  height: 50rpx;
  font-size: 25rpx;
  justify-content: center;
  background-color: rgba(222,184,135, 0.4);
}
.user-info-hint-content{
  color: #fff;
  padding-top: 8rpx;
  padding-left: 20rpx;
  font-weight: bold;
}

/*****中部快速跳转*****/
.pin-cards-container{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  margin: 10rpx 10rpx;
}
/**白盒**/
.pin-card-box{
  margin: 20rpx 40rpx;
  width: 160rpx;
  height: 70rpx;
  border-radius: 40rpx;
  background-color: rgba(255, 255, 255, 0.7);
  box-shadow:12rpx 12rpx 12rpx gainsboro;
}
/**橙字**/
.pin-card-content {
  color: #f7931e;
  font-size: 28rpx;
  font-weight: bold;
  margin: 15rpx auto;
  text-align: center;
  letter-spacing: 5rpx;
  padding-left: 8rpx;
}

.pin-card-content-grey {
  color: #999;
  font-size: 28rpx;
  font-weight: bold;
  margin: 15rpx auto;
  text-align: center;
  letter-spacing: 5rpx;
  padding-left: 8rpx;
}

/***活动通知中心***/
.activity-center-container {
  display: flex;
  flex-direction: column;
  align-items: center;
}
/***活动中心提示语***/
.activity-center-header{
  color: #C0C0C0;
  font-size: 35rpx;
  font-weight: bold;
  margin: 20rpx auto;
  text-align: center;
  letter-spacing: 5rpx;
  text-shadow: 12rpx 12rpx 12rpx gainsboro;
}
/***活动pin card**/
.activity-text-img {
 margin: 30rpx auto;
 width: 650rpx;
 height: 170rpx;
 display: flex;
 flex-direction: row;
 border-radius: 20rpx;
 box-shadow: 4rpx 4rpx 10rpx #999;
 background: rgba(255, 255, 255, 0.4)
}
/***活动名和导航字**/
.activity-text {
 display: flex;
 flex-direction: column;
 justify-content: center;
 margin: 20rpx 20rpx;
}
.activity-slogans {
 display: flex;
 flex-direction: column;
}
.activity-slogan{
 color: #333;
 font-weight: bold;
 font-size: 30rpx;
}
.activity-nav {
  color: #999;
  font-size: 24rpx;
  letter-spacing: 2rpx;
  margin-top: 20rpx;
}

.activity-text-img image{
 width: 50%;
 height: 80%;
 border-radius: 50rpx;
}
